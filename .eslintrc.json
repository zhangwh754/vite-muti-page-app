{
  "root": true,
  "env": {
    "browser": true,
    "es2023": true,
    "node": true
  },
  "extends": ["airbnb-base", "eslint:recommended", "plugin:prettier/recommended", "eslint-config-prettier"],
  "plugins": ["prettier", "html", "eslint-plugin-html"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-new": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "import/no-unresolved": "off",
    "import/no-extraneous-dependencies": "off",
    "func-names": "off"
  },
  "globals": {
    "$": "readonly"
  }
}
